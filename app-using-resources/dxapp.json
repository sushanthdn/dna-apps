{
  "name": "app-using-resources",
  "title": "Sample Application Using Resources",
  "summary": "Sample application that uses executable under resources ",
  "dxapi": "1.0.0",
  "version": "0.0.1",
  "inputSpec": [
    {
      "name": "script_name",
      "label": "Name of the script to run",
      "class": "string",
      "optional": false,
      "help": ""
    }
  ],
  "outputSpec": [
    {
      "name": "outdata",
      "label": "Output data string ",
      "class": "string",
      "help": ""
    }
  ],
  "runSpec": {
    "timeoutPolicy": {
      "*": {
        "hours": 2
      }
    },
    "interpreter": "bash",
    "file": "src/app-using-resources.sh",
    "systemRequirements": {
      "*": {
        "instanceType": "mem1_ssd1_x4"
      }
    },
    "distribution": "Ubuntu",
    "release": "14.04"
  },
  "access": {
    "project": "CONTRIBUTE",
    "network": [
      "*"
    ]
  }
}
